<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.beadando_ea.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <VBox prefHeight="200.0" prefWidth="334.0">
      <children>
         <MenuBar accessibleRole="BUTTON">
           <menus>
             <Menu mnemonicParsing="false" text="File">
               <items>
                 <MenuItem mnemonicParsing="false" text="Close" />
               </items>
             </Menu>
             <Menu mnemonicParsing="false" text="Edit">
               <items>
                     <MenuItem mnemonicParsing="false" text="Create" onAction="#menuCreateClick"/>
                     <MenuItem mnemonicParsing="false" text="Read" onAction="#menuReadClick"/>
                     <MenuItem mnemonicParsing="false" text="Update" onAction="#menuUpdateClick"/>
                 <MenuItem mnemonicParsing="false" text="Delete" onAction="#menuDeleteClick"/>
               </items>
             </Menu>
           </menus>
         </MenuBar>
      </children>
   </VBox>
</VBox>
